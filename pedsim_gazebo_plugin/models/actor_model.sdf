<model name="actor_model">
  <pose>0 0 0 0 0 0</pose>
  <link name="link">
    <collision name="collision_base">
      <pose>0 0 0 0 0 1.57</pose>
      <geometry>
        <box>
          <size>.6 .9 0.001</size>
        </box>
      </geometry>
    </collision>

    <collision name="collision_model">
      <pose>0 0 0 0 0 1.57</pose>
      <geometry>
        <mesh>
          <uri>model://person_walking/meshes/walking.dae</uri>
        </mesh>
      </geometry>
      <!-- To reduce the computations, cancel the stiffness configuration of agents -->
      <surface>
        <contact>
          <ode>
            <kp>9999</kp>
            <kd>1</kd>
          </ode>
        </contact>
      </surface>
      <!-- <surface>
        <contact>
          <collide_without_contact>true</collide_without_contact>
        </contact>
      </surface> -->
    </collision>

    <visual name="visual">
      <pose>0 0 0 0 0 1.57</pose>
      <geometry>
        <mesh>
          <uri>model://person_walking/meshes/walking.dae</uri>
        </mesh>
      </geometry>
    </visual>
  </link>
</model>


